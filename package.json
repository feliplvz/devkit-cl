{
  "name": "devkit-cl",
  "version": "0.1.0",
  "description": "Utilities for Chilean applications. RUT validation, formatters, and local integrations.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.mjs",
      "require": "./dist/validators/index.js"
    },
    "./formatters": {
      "types": "./dist/formatters/index.d.ts",
      "import": "./dist/formatters/index.mjs",
      "require": "./dist/formatters/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "chile",
    "rut",
    "validation",
    "formatter",
    "chilean",
    "latam",
    "typescript",
    "utilities"
  ],
  "author": "Felipe Lopez <felipe@gridlab.tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/feliplvz/devkit-cl.git"
  },
  "bugs": {
    "url": "https://github.com/feliplvz/devkit-cl/issues"
  },
  "homepage": "https://github.com/feliplvz/devkit-cl#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
